{
    "name": "geekish/crap",
    "type": "library",
    "description": "Define aliases for your favorite Composer packages.",
    "keywords": [
        "geekish",
        "crap"
    ],
    "homepage": "https://github.com/geekish/crap",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannah Warmbier",
            "email": "hannahwarmbier@gmail.com",
            "homepage": "https://github.com/geekish",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "composer/composer": "^1.2",
        "mindplay/unbox": "^2.0",
        "symfony/console": "^2.8|^3.0",
        "symfony/process": "^2.5|^3.0",
        "webmozart/json": "^1.2",
        "webmozart/key-value-store": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Geekish\\Crap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geekish\\Crap\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "checkcs": "./vendor/bin/phpcs",
        "fixcs": "./vendor/bin/phpcbf"
    },
    "bin": [
        "bin/crap"
    ],
    "config": {
        "sort-packages": true
    }
}
